{
    "name": "billidev/billiinstaller",
    "description": "Simple laravel application installer with codecanony purchase verification",
    "license": "MIT",
    "minimum-stability": "stable",
    "type": "library",
    "authors": [
        {
            "name": "Muhammad Abrar",
            "email": "billidev27@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "autoload": {
        "psr-4": {
            "Billidev\\Billiinstaller\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Billidev\\BilliInstaller\\Providers\\ProjectInstallerServiceProvider"
            ]
        }
    }

}
