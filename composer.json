{
    "name": "billidev/billi-installer",
    "description": "Simple Laravel web installer with codecanyon product validation",
    "license": "MIT",
    "minimum-stability": "dev",
    "type": "library",
    "authors": [
        {
            "name": "Muhammad Abrar",
            "email": "billidev27@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "autoload": {
        "psr-4": {
            "SdTech\\ProjectInstaller\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },

    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Billidev\\BilliInstaller\\Providers\\ProjectInstallerServiceProvider"
            ]
        }
    }
}
